<section class="todoapp">
	<header class="header">
		<h1>todos</h1>
		<input class="new-todo" placeholder="What needs to be done?" autofocus>
	</header>
	<section class="main">
		<ul class="todo-list">
        <% @lists.each do |list| %>
			<li>
				<div class="view">
					<label><%= link_to list.name, list_path(list) %></label>
				</div>
			</li>
		<% end %>
		</ul>
	</section>
	<footer class="footer">
	</footer>
</section>